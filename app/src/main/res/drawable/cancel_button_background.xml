<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <size
                android:width="80dp"
                android:height="80dp" />
            <solid android:color="#00FFFFFF" />
            <corners android:radius="40dp" />
            <stroke
                android:width="5dp"
                android:color="#FFFFFF" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="#FFFFFF" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="#FFFFFF" />
            </shape>
        </rotate>
    </item>
</layer-list>
